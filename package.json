{
  "name": "ryantxu-ajax-panel",
  "version": "0.0.6-dev",
  "description": "AJAX Panel Plugin for Grafana",
  "main": "src/module.ts",
  "scripts": {
    "build": "webpack --config webpack.config.prod.js",
    "dev": "webpack --mode development",
    "watch": "webpack --mode development --watch",
    "test": "jest --config jest.config.js",
    "precommit": "pretty-quick --staged",
    "pretty": "prettier --write src/**/*.ts"
  },
  "keywords": ["ajax", "grafana", "plugin", "panel", "webcamera"],
  "author": "ryantxu",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ryantxu/ajax-panel.git"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5",
    "useTabs": false,
    "printWidth": 90,
    "bracketSpacing": false,
    "semi": true
  },
  "dependencies": {
    "immutable": "^3.8.2",
    "lodash": "^4.17.4",
    "moment": "^2.22.1",
    "papaparse": "^4.5.0",
    "prismjs": "^1.6.0",
    "slate": "^0.33.7",
    "slate-plain-serializer": "^0.5.13"
  },
  "devDependencies": {
    "@types/grafana": "https://git@github.com/CorpGlory/types-grafana.git",
    "@types/jest": "^22.2.3",
    "@types/lodash": "^4.14.74",
    "babel-core": "^6.26.3",
    "babel-jest": "^23.0.1",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.7.0",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.5.1",
    "css-loader": "^0.28.11",
    "grafana-sdk-mocks": "github:grafana/grafana-sdk-mocks",
    "jest": "^23.0.1",
    "ng-annotate-webpack-plugin": "^0.3.0",
    "style-loader": "^0.21.0",
    "prettier": "1.10.2",
    "pretty-quick": "^1.4.1",
    "ts-jest": "^22.4.6",
    "ts-loader": "^4.3.0",
    "typescript": "^2.8.3",
    "webpack": "^4.9.1",
    "webpack-cli": "^2.1.4"
  }
}
